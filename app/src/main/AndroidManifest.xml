<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.NGO.weforyou" >

    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logopng"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/logopng"
        android:supportsRtl="true"
        android:theme="@style/Theme.WeForYou" >
        <activity
            android:name=".Options"
            android:exported="true" >
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
        </activity>
        <activity
            android:name=".urban"
            android:exported="true" >
        </activity>
        <activity android:name=".Update" />
        <activity
            android:name=".Main2Activity"
            android:label="@string/title_activity_main2"
            android:theme="@style/Theme.WeForYou.NoActionBar" />
        <activity
            android:name=".HomeActivity"
            android:theme="@style/homeActivity" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>